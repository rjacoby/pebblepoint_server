extends layout

block content
  .container.theme-showcase(role="main")
    .jumbotron.hidden-xs
      h1= title
      p Use #{title} to control your presentation
    #flash.alert.alert-success(style="display:none;")
    .page-header
      h1 Controls
    .btn-group
      button.btn.btn-lg.btn-info.ppt-button(type="button", data-url="/go/previous") Prev
      button.btn.btn-lg.btn-primary.ppt-button(type="button", data-url="/go/next") Next
